[{"id": 26366, "title": "Launch Roundup: Vulcan ready for second flight; Hera mission set to study asteroid", "url": "https://www.nasaspaceflight.com/2024/09/launch-roundup-093024/", "image_url": "https://www.nasaspaceflight.com/wp-content/uploads/2024/09/GYvVA8gXAAAAxKx-1170x780.jpg", "news_site": "NASASpaceflight", "summary": "The United Launch Alliance (ULA) is preparing for the second flight of its Vulcan Centaur rocket this week. The mission will see Vulcan carry an inert payload to orbit alongside some experiments and demonstrations and will complete its certification process with the U.S. Space Force (USSF).", "published_at": "2024-09-30T21:22:09Z", "updated_at": "2024-10-02T06:47:47.058445Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}, {"launch_id": "cd199e27-63cd-409c-817c-5c38039b17cd", "provider": "Launch Library 2"}, {"launch_id": "9d576892-dcf0-472b-92d1-37053ff549ab", "provider": "Launch Library 2"}], "events": []}, {"id": 26353, "title": "Hera asteroid mission", "url": "https://www.esa.int/Space_Safety/Hera", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2015/06/hera_scans_dimorphos_with_its_laser_altimeter/15477278-17-eng-GB/Hera_scans_Dimorphos_with_its_laser_altimeter_card_full.png", "news_site": "ESA", "summary": "Hera asteroid mission", "published_at": "2024-09-30T15:37:00Z", "updated_at": "2024-09-30T15:07:45.421168Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}], "events": []}, {"id": 26346, "title": "Nearing Hera era in space", "url": "https://www.esa.int/ESA_Multimedia/Images/2024/09/Nearing_Hera_era_in_space", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2024/09/nearing_hera_era_in_space/26331951-1-eng-GB/Nearing_Hera_era_in_space_card_full.png", "news_site": "ESA", "summary": "Nearing Hera era in space", "published_at": "2024-09-30T10:32:00Z", "updated_at": "2024-09-30T12:58:53.378738Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}], "events": []}, {"id": 26313, "title": "View from Hera\u2019s Asteroid Deck", "url": "https://www.esa.int/ESA_Multimedia/Images/2024/09/View_from_Hera_s_Asteroid_Deck", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2024/09/view_from_hera_s_asteroid_deck/26330511-1-eng-GB/View_from_Hera_s_Asteroid_Deck_card_full.jpg", "news_site": "ESA", "summary": "View from Hera\u2019s Asteroid Deck", "published_at": "2024-09-27T13:41:00Z", "updated_at": "2024-09-27T16:17:35.137041Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}], "events": []}, {"id": 26289, "title": "Look inside ESA\u2019s Hera asteroid mission", "url": "https://www.esa.int/ESA_Multimedia/Videos/2024/09/Look_inside_ESA_s_Hera_asteroid_mission", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/videos/2024/09/look_inside_esa_s_hera_asteroid_mission/26328018-1-eng-GB/Look_inside_ESA_s_Hera_asteroid_mission_card_full.png", "news_site": "ESA", "summary": "Hera is ESA's first planetary defence mission, set to probe the lingering mysteries of a unique target among the 1.3 million known asteroids of our Solar System: the first body to have had its orbit shifted by human action.", "published_at": "2024-09-26T14:52:00Z", "updated_at": "2024-09-26T13:49:16.975073Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}], "events": []}, {"id": 26190, "title": "Hera planetary defence mission: solving asteroid mysteries", "url": "https://www.esa.int/ESA_Multimedia/Videos/2024/09/Hera_planetary_defence_mission_solving_asteroid_mysteries", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/videos/2024/09/hera_planetary_defence_mission_solving_asteroid_mysteries/26319695-1-eng-GB/Hera_planetary_defence_mission_solving_asteroid_mysteries_card_full.png", "news_site": "ESA", "summary": "There\u2019s a mystery out there in deep space \u2013 and solving it will make Earth safer. That\u2019s why the European Space Agency\u2019s Hera mission is taking shape \u2013 to go where one particular spacecraft has gone before.", "published_at": "2024-09-23T13:09:00Z", "updated_at": "2024-09-24T06:34:40.569310Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}], "events": []}, {"id": 26105, "title": "Hera asteroid mission launch kit", "url": "https://www.esa.int/Space_Safety/Hera/Hera_asteroid_mission_launch_kit", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2024/09/hera_launch_kit/26317680-1-eng-GB/Hera_Launch_Kit_card_full.jpg", "news_site": "ESA", "summary": "Download this Hera launch kit to learn more about the mission's planetary defence, technology demonstration and bonus science goals.", "published_at": "2024-09-18T13:53:00Z", "updated_at": "2024-09-18T14:23:57.335789Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}], "events": []}, {"id": 25931, "title": "Hera CubeSats\u2019 touchdown", "url": "https://www.esa.int/ESA_Multimedia/Images/2024/09/Hera_CubeSats_touchdown", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2024/09/hera_cubesats_touchdown/26307586-1-eng-GB/Hera_CubeSats_touchdown_card_full.jpg", "news_site": "ESA", "summary": "Hera CubeSats\u2019 touchdown", "published_at": "2024-09-11T15:22:00Z", "updated_at": "2024-09-12T06:34:09.806746Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}], "events": []}, {"id": 25722, "title": "Goodbye Hera: asteroid mission departs ESA test centre", "url": "https://www.esa.int/Space_Safety/Hera/Goodbye_Hera_asteroid_mission_departs_ESA_test_centre", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2024/09/hera_departs_esa_test_centre/26294184-1-eng-GB/Hera_departs_ESA_test_centre_card_full.jpg", "news_site": "ESA", "summary": "After a year of testing, ESA\u2019s Hera asteroid mission for planetary defence\u00a0is about to depart Europe and head towards its launch site in the USA. The Hera team looked on as the crated spacecraft \u2013 along with its twin miniature CubeSats and additional equipment \u2013 was driven away from ESA\u2019s ESTEC Test Centre in Noordwijk, the Netherlands.", "published_at": "2024-09-02T10:00:00Z", "updated_at": "2024-09-02T11:51:41.628044Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}], "events": []}, {"id": 25005, "title": "Glimpses of Hera\u2019s target asteroids inspire new science", "url": "https://www.esa.int/Space_Safety/Hera/Glimpses_of_Hera_s_target_asteroids_inspire_new_science", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2019/08/hera_glides_past_didymos_to_dimorphos/19660129-7-eng-GB/Hera_glides_past_Didymos_to_Dimorphos_card_full.png", "news_site": "ESA", "summary": "As ESA\u2019s Hera mission for planetary defence completes its pre-launch testing, its target bodies have come into focus as tiny worldlets of their own. A special issue of Nature Communications\u00a0published this week presents studies of the Didymos asteroid and its Dimorphos moon, based on the roughly five and a half minutes of close-range footage returned by NASA\u2019s DART spacecraft before it impacted the latter body \u2013 along with post-impact images from the Italian Space Agency\u2019s LICIACube.", "published_at": "2024-07-31T15:37:00Z", "updated_at": "2024-07-31T14:33:43.166465Z", "featured": false, "launches": [{"launch_id": "e1ffdd01-c146-44d0-a250-b845dc2eb20d", "provider": "Launch Library 2"}], "events": []}]