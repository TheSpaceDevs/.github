[{"id": 30886, "title": "Biomass: from Europe to French Guiana", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/ESA_Multimedia/Videos/2025/04/Biomass_from_Europe_to_French_Guiana", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/videos/2025/04/biomass_from_europe_to_french_guiana/26680148-1-eng-GB/Biomass_from_Europe_to_French_Guiana_card_full.png", "news_site": "ESA", "summary": "ESA\u2019s Biomass satellite has been at Europe\u2019s Spaceport in French Guiana since early March, undergoing final preparations for launch aboard a Vega-C rocket. This timelapse video captures key stages of its journey \u2014 from its arrival in Kourou to its installation in the launch tower.", "published_at": "2025-04-28T11:00:00Z", "updated_at": "2025-04-28T14:20:21.084135Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}, {"id": 30884, "title": "Biomass poised for liftoff to unveil forest secrets", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/Applications/Observing_the_Earth/FutureEO/Biomass/Biomass_poised_for_liftoff_to_unveil_forest_secrets", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2025/04/biomass_ready_for_liftoff/26675081-1-eng-GB/Biomass_ready_for_liftoff_card_full.jpg", "news_site": "ESA", "summary": "After years of careful design and preparation, ESA\u2019s Earth Explorer Biomass satellite is set for launch tomorrow, 29 April at 11:15 CEST, aboard a Vega-C rocket from Europe\u2019s Spaceport in French Guiana.", "published_at": "2025-04-28T07:00:00Z", "updated_at": "2025-04-28T08:53:35.564485Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}, {"id": 30819, "title": "Watch Biomass launch live", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/Applications/Observing_the_Earth/FutureEO/Biomass/Watch_Biomass_launch_live", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2025/04/vega-c_fairing_release_revealing_biomass/26664419-1-eng-GB/Vega-C_fairing_release_revealing_Biomass_card_full.png", "news_site": "ESA", "summary": "ESA's Biomass mission is on the launch pad at Europe\u2019s Spaceport in Kourou, French Guiana. Don\u2019t miss the live coverage of liftoff on Tuesday 29 April, on ESA WebTV. Coverage will start at 10:55 CEST with commentary live from Kourou and ESA\u2019s European Space Operations Centre in Germany. The Vega-C rocket carrying Biomass is scheduled for liftoff at 11:15.", "published_at": "2025-04-24T09:50:00Z", "updated_at": "2025-04-24T11:08:51.517914Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}, {"id": 30818, "title": "Simulations ongoing for Biomass launch", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/ESA_Multimedia/Images/2025/04/Simulations_ongoing_for_Biomass_launch", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2025/04/simulations_ongoing_for_biomass_launch/26674135-1-eng-GB/Simulations_ongoing_for_Biomass_launch_card_full.png", "news_site": "ESA", "summary": "From the ESA Blogs.", "published_at": "2025-04-23T14:53:00Z", "updated_at": "2025-04-24T11:08:41.954758Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}, {"id": 30804, "title": "Final preparations for Biomass operations", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/Enabling_Support/Operations/Final_preparations_for_Biomass_operations", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2025/04/simulations_ongoing_for_biomass_launch/26674135-1-eng-GB/Simulations_ongoing_for_Biomass_launch_card_full.png", "news_site": "ESA", "summary": "As the launch of the Biomass satellite approaches, the excitement is palpable at ESA\u2019s European Space Operations Centre (ESOC) in Darmstadt, Germany. Over the winter months, a dedicated team has been meticulously preparing for the satellite's critical \"Launch and Early Orbit\" phase (LEOP).", "published_at": "2025-04-23T14:38:00Z", "updated_at": "2025-04-24T11:08:38.032884Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}, {"id": 30674, "title": "Forest mission sealed within rocket fairing for liftoff", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/Applications/Observing_the_Earth/FutureEO/Biomass/Forest_mission_sealed_within_rocket_fairing_for_liftoff", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2025/04/sealing_biomass_within_the_vega-c_fairing/26664607-2-eng-GB/Sealing_Biomass_within_the_Vega-C_fairing_card_full.jpg", "news_site": "ESA", "summary": "With the launch of ESA\u2019s Biomass satellite scheduled for 29 April, preparations at Europe\u2019s Spaceport in Kourou, French Guiana, have reached a key milestone. The satellite has now been sealed inside the protective fairing of the Vega-C rocket \u2013 now hidden from view, the satellite is almost ready for its journey into space.", "published_at": "2025-04-16T11:45:00Z", "updated_at": "2025-04-17T09:17:13.389909Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}, {"id": 30572, "title": "Biomass fully loaded", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/Applications/Observing_the_Earth/FutureEO/Biomass/Biomass_fully_loaded", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2025/04/preparing_biomass_for_fuelling/26655603-1-eng-GB/Preparing_Biomass_for_fuelling_card_full.png", "news_site": "ESA", "summary": "Marking a major milestone in the preparation of ESA\u2019s Biomass satellite for its scheduled 29 April liftoff, experts have completed the critical and hazardous process of fuelling the satellite.", "published_at": "2025-04-10T09:30:00Z", "updated_at": "2025-04-11T06:17:50.473095Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}, {"id": 30305, "title": "Biomass cleared for fuelling", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/ESA_Multimedia/Images/2025/03/Biomass_cleared_for_fuelling", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2025/03/biomass_cleared_for_fuelling/26642387-1-eng-GB/Biomass_cleared_for_fuelling_card_full.jpg", "news_site": "ESA", "summary": "From the ESA Blogs.", "published_at": "2025-03-31T14:39:00Z", "updated_at": "2025-03-31T16:37:24.534625Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}, {"id": 29988, "title": "Biomass out of the box", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/ESA_Multimedia/Images/2025/03/Biomass_out_of_the_box", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2025/03/biomass_out_of_the_box/26619597-1-eng-GB/Biomass_out_of_the_box_card_full.jpg", "news_site": "ESA", "summary": "From the ESA Blogs.", "published_at": "2025-03-17T11:25:00Z", "updated_at": "2025-03-18T12:28:42.337990Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}, {"id": 29805, "title": "ESA\u2019s pioneering Biomass satellite arrives at launch site", "authors": [{"name": "ESA", "socials": null}], "url": "https://www.esa.int/Applications/Observing_the_Earth/FutureEO/Biomass/ESA_s_pioneering_Biomass_satellite_arrives_at_launch_site", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2025/03/biomass_heads_for_the_launch_site/26607491-1-eng-GB/Biomass_heads_for_the_launch_site_card_full.jpg", "news_site": "ESA", "summary": "Marking another step towards new insights into Earth\u2019s forests and their role in the carbon cycle, ESA\u2019s groundbreaking Biomass satellite has arrived at Europe's Spaceport in French Guiana, to be prepared for liftoff on a Vega-C rocket at the end of April.", "published_at": "2025-03-10T10:00:00Z", "updated_at": "2025-03-10T16:38:37.300456Z", "featured": false, "launches": [{"launch_id": "ccbd07da-d3c7-4b22-a5ee-cb190f6e06b0", "provider": "Launch Library 2"}], "events": []}]