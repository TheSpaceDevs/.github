[{"id": 24481, "title": "Eight CubeSats Lift Off for NASA on Firefly Aerospace Rocket!", "url": "https://www.nasa.gov/image-article/eight-cubesats-lift-off-for-nasa-on-firefly-aerospace-rocket/", "image_url": "https://www.nasa.gov/wp-content/uploads/2024/07/launch-credit-firefly-aerospace-trevor-mahlmann.jpg?w=800", "news_site": "NASA", "summary": "As part of NASA\u2019s CubeSat Launch Initiative, Firefly Aerospace launched eight small satellites on July 3 aboard the company\u2019s Alpha rocket. Named \u201cNoise of Summer,\u201d the rocket successfully lifted off from Space Launch Complex 2 at Vandenberg Air Force Base in California at 9:04 p.m. PDT. The CubeSat missions were designed by universities and NASA [&#8230;]", "published_at": "2024-07-04T18:04:30Z", "updated_at": "2024-07-04T18:12:22.437401Z", "featured": false, "launches": [], "events": []}, {"id": 24478, "title": "Ariane 6 first flight timeline", "url": "https://www.esa.int/ESA_Multimedia/Images/2024/07/Ariane_6_first_flight_timeline", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2024/07/ariane_6_first_flight_timeline/26212783-1-eng-GB/Ariane_6_first_flight_timeline_card_full.jpg", "news_site": "ESA", "summary": "Ariane 6 first flight timeline", "published_at": "2024-07-04T17:21:00Z", "updated_at": "2024-07-04T15:32:23.654245Z", "featured": false, "launches": [], "events": []}, {"id": 24479, "title": "Ariane 6 launch: how to watch and what to look out for", "url": "https://www.esa.int/Enabling_Support/Space_Transportation/Ariane/Ariane_6_launch_how_to_watch_and_what_to_look_out_for", "image_url": "https://www.esa.int/var/esa/storage/images/esa_multimedia/images/2024/06/ariane_6_key_visual_-_landscape/26132475-1-eng-GB/Ariane_6_key_visual_-_landscape_card_full.jpg", "news_site": "ESA", "summary": "Ariane 6 key visual", "published_at": "2024-07-04T17:09:00Z", "updated_at": "2024-07-04T15:52:20.032946Z", "featured": false, "launches": [], "events": []}, {"id": 24480, "tit